{% extends 'base.html.twig' %}

{% block body %}
    <h1>Typeequipement</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ typeequipement.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ typeequipement.nom }}</td>
            </tr>
            <tr>
                <th>Israyonnage</th>
                <td>{% if typeequipement.isRayonnage %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('typeequipement_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('typeequipement_edit', { 'id': typeequipement.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
